<Window x:Class="Home_managment.user"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Бланк реєстрації" Height="264.776" Width="300" Closed="Window_Closed_1">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Menu x:Name="Menu" HorizontalAlignment="Left" Height="46" VerticalAlignment="Top" Width="473">
            <Button Content="Про нас" Height="36" Width="61" Click="Button_Click_1"/>
            <Button Content="Способи оплати" Height="36" Width="109" Click="Button_Click_2"/>
        </Menu>
        <Button x:Name="Print" Content="Заповнити бланк" HorizontalAlignment="Left" Height="57" Margin="7,167,0,0" VerticalAlignment="Top" Width="272" RenderTransformOrigin="0.473,0.337" Click="Print_Click"/>
        <TextBlock x:Name="Message" HorizontalAlignment="Left" Height="103" Margin="10,59,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="272"><InlineUIContainer>
        		<WebBrowser x:Name="Shower" Height="101" Width="234"/>
        	</InlineUIContainer></TextBlock>

    </Grid>
</Window>
