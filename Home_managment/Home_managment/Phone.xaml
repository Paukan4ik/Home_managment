<Window x:Class="Home_managment.Phone"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Підтвердження" Height="158.209" Width="344.777" WindowStartupLocation="CenterScreen" Closed="Window_Closed_1">
    <Grid>
        <TextBox x:Name="phone" HorizontalAlignment="Right" Height="44" Margin="0,10,22,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="290" VerticalContentAlignment="Center"/>
        <Button x:Name="Go" Content="Увійти в панель користувача" HorizontalAlignment="Left" Height="44" Margin="22,74,0,0" VerticalAlignment="Top" Width="290" VerticalContentAlignment="Center" Click="Button_Click_1"/>
        <Button x:Name="Print" Content="Друк" HorizontalAlignment="Left" Height="100" Margin="-22,158,0,-131" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="Preview" Content="Попередній перегляд" HorizontalAlignment="Left" Height="100" Margin="128,158,0,-131" VerticalAlignment="Top" Width="75"/>
        <TextBlock x:Name="label" HorizontalAlignment="Left" Height="188" Margin="-35,143,-202,-190" TextWrapping="Wrap" VerticalAlignment="Top" Width="272"/>

    </Grid>
</Window>
